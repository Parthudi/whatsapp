{
    "rules": {
      "class-name": true,
      "comment-format": [
        true,
        "check-space"
      ],
      "indent": [
        true,
        "spaces"
      ],
      "no-var-keyword": true,
      "quotemark": [
        true,
        "double",
        "avoid-escape"
      ],
      "semicolon": [
        true,
        "always",
        "ignore-bound-class-methods"
      ],
      "no-internal-module": false,
      "prefer-const": true,
      "jsdoc-format": true
    }
  }